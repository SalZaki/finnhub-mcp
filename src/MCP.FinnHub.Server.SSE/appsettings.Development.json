{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FinnHub": {
    "ApiKey": "your-api-key-here",
    "BaseUrl": "https://finnhub.io/api/v1",
    "TimeoutSeconds": 10,
    "Endpoints": [
      {
        "Name": "Quote",
        "Url": "/quote",
        "IsActive": true,
        "Description": "Get real-time quote for a stock symbol"
      },
      {
        "Name": "News",
        "Url": "/news",
        "IsActive": false,
        "Description": "Fetch general financial news"
      },
      {
        "Name": "Crypto",
        "Url": "/crypto/symbol",
        "IsActive": true,
        "Description": "Retrieve list of supported cryptocurrency symbols"
      }
    ]
  }
}
